mkdir "weaam ahmad"
cd "weaam ahmad"

mkdir Documents
cd Documents
touch file1.txt file2.txt file3.txt
ls

cd ..
touch .hidden_file
ls -a

nano Documents/file1.txt
Hello massage
# then press ctrl + o to save and ctrl + x to exit Nano

cat Documents/file1.txt

cd Documents

mkdir "-"
cd "-"

cp -r Documents My_Documents

cat .hidden_file

mv Documents/file2.txt . Weaam ahmad
mv Documents/file3.txt . Weaam ahmad


cd ..
rm -r "weaam ahmad"
